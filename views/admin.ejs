<!DOCTYPE html>
<html lang="en">
    <%- include('partials/head') %>
<body>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">No</th>
            <th scope="col">ID</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">option</th>
          </tr>
        </thead>
        <tbody>
         <% for(let i = 0; i < user.length; i++) {%>
        
            <tr>
              <th scope="row"><%= i + 1 %></th>
              <td><%= user[i].id %></td>
              <td><%= user[i].first_name %></td>
              <td><%= user[i].last_name %></td>
              <td><a href="#"></a></td>
            </tr>
        
        <% } %>
         
        </tbody>
      </table>
</body>
</html>